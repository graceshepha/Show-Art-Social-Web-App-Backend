{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "./data/",
            "./utils/",
            "./routes/",
            "./README.md"
        ],
        "exclude": ["./node_modules/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}
